  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pennyren/reactEMS/issues"
  },
  "homepage": "https://github.com/pennyren/reactEMS#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "pg": "^6.1.0",
    "react": "^15.3.1",
    "react-addons-transition-group": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
